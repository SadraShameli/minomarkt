{
    "name": "web-minomarkt",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "development": "npm run dev",
        "eslint": "eslint \"assets/**/*.ts\"",
        "eslint:fix": "eslint --fix \"assets/**/*.ts\"",
        "format": "prettier --check \"assets/**/*.ts\"",
        "format:write": "prettier --write \"assets/**/*.ts\"",
        "lint": "npm run format && npm run eslint && npm run stylelint",
        "lint:fix": "npm run format:write && npm run eslint:fix && npm run stylelint:fix",
        "prepare": "husky",
        "preview": "npm run build && npm run start",
        "prod": "npm run build",
        "production": "npm run build",
        "start": "npm run dev",
        "stylelint": "stylelint \"assets/**/*.scss\"",
        "stylelint:fix": "stylelint --fix \"assets/**/*.scss\"",
        "typecheck": "tsc --noEmit",
        "update": "npx ncu --interactive --format group",
        "watch": "npm run dev"
    },
    "dependencies": {
        "bootstrap": "^5.3.3",
        "countup.js": "^2.8.0",
        "swiper": "11.2.5"
    },
    "devDependencies": {
        "@types/google.maps": "^3.58.1",
        "@types/node": "^22.13.10",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "dotenv": "^16.4.7",
        "eslint": "9.22.0",
        "eslint-plugin-perfectionist": "^4.10.1",
        "husky": "^9.1.7",
        "prettier": "3.5.3",
        "sass-embedded": "1.85.1",
        "stylelint": "16.16.0",
        "stylelint-config-recommended-scss": "^14.1.0",
        "stylelint-order": "^6.0.4",
        "vite": "6.2.2"
    }
}
